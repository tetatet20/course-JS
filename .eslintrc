{
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "requireConfigFile": false,
      "ecmaVersion": "latest"
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
        
    },
    "rules": {
        "semi": "off",
        "indent": "off",
        "require-jsdoc": "off",
        "operator-linebreak":"off"
        
    },
    "extends": [
        "eslint:recommended",
        "google"
    ]
}